{
  "name": "design-pattern-js",
  "version": "1.0.0",
  "description": "Design patterns implemented in javascript",
  "scripts": {
    "build-docs": "node ./build-docs.js",
    "lint": "eslint --cache --fix src/**/*.js",
    "test": "cross-env NODE_ENV=test nyc mocha --ui bdd src/**/__tests__/**/*.js --exit",
    "test:watch": "cross-env NODE_ENV=test nyc mocha src/**/__tests__/**/*.js --exit -w",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.5.0",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "fs-extra-promise": "^1.0.1",
    "ghooks": "^2.0.4",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0",
    "prettier": "^1.16.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    }
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helmuthdu/design-pattern-js.git"
  },
  "keywords": [
    "design",
    "patterns",
    "gof"
  ],
  "author": "Helmuth Saatkamp<helmuthdu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helmuthdu/design-pattern-js/issues"
  },
  "homepage": "https://github.com/helmuthdu/design-pattern-js#readme"
}
